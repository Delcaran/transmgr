{
    "eth0_ip": "192.168.1.1",
    "drives": [
        "hdd1/.alive",
        "hdd2/.alive"
    ],
    "files": {
        "start": ".torrent/force_torrent",
        "stop": ".torrent/stop_torrent",
        "pid": ".torrent/mgr",
        "transpid": ".config/trans.pid",
        "bgscript": "scripts/runbg.sh"
    },
    "rpc": {
        "host": "127.0.0.1",
        "port": "9091",
        "user": "usr",
        "pass": "pwd",
        "socket": "123"
    },
    "schedule": {
        "week": {
            "start": {
                "hour": 0,
                "min": 30
            },
            "end": {
                "hour": 17,
                "min": 0
            }
        },
        "weekend": {
            "start": {
                "hour": 2,
                "min": 30
            },
            "end": {
                "hour": 7,
                "min": 0
            }
        }
    },
    "commands": {
        "vpn_start": "sudo /usr/sbin/openvpn ",
        "vpn_stop": "sudo killall openvpn"
    },
    "private_trackers": [
        "idoftracker"
    ]
}